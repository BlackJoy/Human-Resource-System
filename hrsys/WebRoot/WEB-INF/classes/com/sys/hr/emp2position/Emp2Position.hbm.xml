<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="com.sys.hr.emp2position">
	<class name="Emp2Position" table="TBL_EMPLOYEE2POSITION">
		<id name="id" column="id">
			<generator class="uuid" />
		</id>
		
		<property name="employeeId" column="EMPLOYEE_ID" />
		<property name="orgId" column="ORG_ID" />
		<property name="positionId" column="POSITION_ID" />
		<property name="positionType" column="POSITIONTYPE" />
		<property name="startTime" column="STARTTIME" />
		<property name="endTime" column="ENDTIME" />
		<property name="positionStatus" column="POSITIONSTATUS" />
	</class>
</hibernate-mapping>
