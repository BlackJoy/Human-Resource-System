<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="com.sys.approve">
	<class name="ApproveExec" table="TBL_APPROVE_EXEC">
		<id name="id" column="id">
			<generator class="uuid" />
		</id>
		
		<property name="approveId" column="APPROVEID" />
		<property name="approveName" column="APPROVENAME" />
		<property name="startTime" column="STARTTIME" />
		<property name="endTime" column="ENDTIME" />
		<property name="status" column="STATUS" />
		<property name="contentURL" column="CONTENTURL" />
		<property name="contentID" column="CONTENTID" />
		<property name="isoverandsave" column="ISOVERANDSAVE" />
	</class>
</hibernate-mapping>
