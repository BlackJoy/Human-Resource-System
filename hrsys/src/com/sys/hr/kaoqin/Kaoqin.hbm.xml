<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="com.sys.hr.kaoqin">
	<class name="Kaoqin" table="TBL_KAOQIN">
		<id name="id" column="ID">
			<generator class="uuid" />
		</id>
		<property name="employeeName" column="EMPLOYEENAME" />
		<property name="kaoqinTime" column="KAOQIN_TIME" />
		<property name="state" column="STATE" />
		<property name="isadd" column="ISADD" />
		<property name="orgid" column="ORGID" />
		<property name="postname" column="POSTNAME" />
	</class>
</hibernate-mapping>
