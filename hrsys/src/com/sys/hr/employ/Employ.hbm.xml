<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="com.sys.hr.employ">
	<class name="Employ" table="TBL_Employ">
		<id name="id" column="id">
			<generator class="uuid" />
		</id>
		
		<property name="name" column="name" />
		<property name="gender" column="gender" />
		<property name="identityCard" column="identityCard" />
		<property name="education" column="education" />
		<property name="specialty" column="specialty" />
		<property name="graduateSchool" column="graduateSchool" />
		<property name="address" column="address" />
		<property name="phone" column="phone" />
		<property name="orgid" column="orgid" />
		<property name="position" column="position" />
		<property name="yingpinTime" column="yingpinTime" />
		<property name="shiyongTime" column="shiyongTime" />
		<property name="ruzhiTime" column="ruzhiTime" />
		<property name="employStatus" column="employStatus" />
	</class>
</hibernate-mapping>
